<script language="JavaScript">
$(function(){
	$("#newCourses").click(function(){
		$(this).addClass("active").prev().removeClass("active");
		$("#showNewCourses").show();
		$("#showOldCourses").hide();
	});
	$("#oldCourses").click(function(){
		$(this).addClass("active").next().removeClass("active");
		$("#showOldCourses").show();
		$("#showNewCourses").hide();
	});
});
</script>
